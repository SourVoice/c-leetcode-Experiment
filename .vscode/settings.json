{
  "files.associations": {
    "*.img": "hexdump",
    "alloc.h": "c",
    "iostream": "cpp",
    "stdio.h": "c",
    "assert.h": "c",
    "math.h": "c",
    "initializer_list": "cpp",
    "type_traits": "cpp",
    "xstring": "cpp",
    "xutility": "cpp",
    "creat_printlist.h": "c",
    "ostream": "cpp",
    "limits": "cpp",
    "*.tcc": "cpp",
    "istream": "cpp",
    "streambuf": "cpp",
    "string": "cpp",
    "iosfwd": "cpp",
    "xmemory": "cpp",
    "algorithm": "cpp",
    "stack": "cpp",
    "map": "cpp",
    "atomic": "cpp",
    "deque": "cpp",
    "iterator": "cpp",
    "sstream": "cpp",
    "vector": "cpp",
    "xlocale": "cpp",
    "xlocnum": "cpp",
    "xiosbase": "cpp",
    "xhash": "cpp",
    "bits/stdc++.h": "cpp",
    "postfix_evaluator.h": "c",
    "unordered_map": "c"
  },
  "code-runner.executorMap": {
    "cpp": "cd $dir && g++ \"$fileName\" -std=c++17 -finput-charset=GBK -fexec-charset=utf-8 -I D:/workstation/sdks/boost_1_81_0_mingw_build/include/boost-1_81 -L D:/workstation/sdks/boost_1_81_0_mingw_build/lib -D LOCAL -o \"$workspaceRoot\\exe\\$fileNameWithoutExt.exe\" && \"$workspaceRoot\\exe\\$fileNameWithoutExt.exe\" && del \"$workspaceRoot\\exe\\$fileNameWithoutExt.exe\"",
    "c": " cd $dir && gcc $fileName  -finput-charset=GBK -fexec-charset=utf-8 -D LOCAL -o $workspaceRoot\\exe\\$fileNameWithoutExt.exe && $workspaceRoot\\exe\\$fileNameWithoutExt.exe",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt"
  },
  "asoul.reminderViewIntervalInMinutes": 30,
  "files.encoding": "gbk",
  "zhihu.enableMermaidToPng": true,
  "zhihu.mermaidTheme": "dark", // 支持设置 mermaid 图片主题，默认值是 "default"
  "vscode-office.autoTheme": true,
  "workbench.editorAssociations": {
    "*.md": "default"
  },
  "jupyter.kernels.filter": [],
  "jupyter.themeMatplotlibPlots": true,
  "workbench.colorCustomizations": {
    "editorIndentFuide.background": "#ff0000"
  },
  "python.formatting.provider": "yapf"
}